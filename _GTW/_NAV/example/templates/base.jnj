{% extends "html/base.jnj" %}
{%- import "html/form.jnj" as Form %}
{%- block navigate -%}
  <h1>H1 BN</h1>
  {{ super () }}
  {{ page.abs_href }}
  {%- if request.user.authenticated %}
    <a href="/logout.html">{{ request.user.username }}, logout</a>
  {%- else %}
    {{ Form.login_inline (action = "/login/", next = page.abs_href) }}
  {% endif -%}
{%- endblock -%}
